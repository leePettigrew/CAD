  </head>
  <body>
    
    <%= render 'home/header' %>


    <br><br>
    <div class ="container" >

              <% if notice %>
                <%= render'layouts/alerts' %>
              <% end %>


       <%= yield %>
    </div>
   


    





    
   
  </body>
</html>